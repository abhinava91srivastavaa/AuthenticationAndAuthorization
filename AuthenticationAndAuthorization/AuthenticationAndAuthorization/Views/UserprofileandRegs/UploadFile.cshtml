
@{
                /**/

                ViewBag.Title = "UploadFile";
                Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>UploadFile</h2>

@using (Html.BeginForm("UploadFile", "UserprofileandRegs", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @*<input type="file" name="postedFile" />
    <input type="submit" value="Import" />*@


    <div class="form-horizontal">
        <h4>Upload File</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
       
        <div class="form-group">
            @Html.Label("Upload File", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                <input type="file" name="postedFile" class="control-label col-md-5" />
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Import" class="btn btn-primary" />
            </div>
        </div>
    </div>
}